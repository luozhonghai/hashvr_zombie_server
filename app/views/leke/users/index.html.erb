<h1>Leke::Users#index</h1>
<p>Find me in app/views/leke/users/index.html.erb</p>

<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>Id</th>
      <th>Ip</th>
      <th colspan="3">Time</th>
    </tr>
  </thead>

  <tbody>
    <% @leke_users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.ip %></td>
        <td><%= user.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @leke_users %>
