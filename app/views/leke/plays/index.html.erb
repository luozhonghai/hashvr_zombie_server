<h1>Leke::Plays#index</h1>
<p>Find me in app/views/leke/plays/index.html.erb</p>
<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>Id</th>
      <th>UserIp</th>
      <th colspan="3">PlayTime</th>
    </tr>
  </thead>

  <tbody>
    <% @leke_plays.each do |play| %>
      <tr>
        <td><%= play.id %></td>
        <td><%= play.leke_user.ip %></td>
        <td><%= play.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @leke_plays %>